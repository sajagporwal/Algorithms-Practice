
public class bubble {
	public static void main(String[] args)
	{
		int a[]={5,3,4,1,2};
		bubbles(a,5);
		for(int i=0;i<6;i++)
		{
			System.out.println(a[i] );
		}
	}
	public static void bubbles(int a[],int high)
	{
		int i,j,temp;
		int k=0;
		for(i=0;i<=high;i++)
		{
			
			for(j=0;j<high;j++)
			{
				k=j+1;
				if(a[j]>a[k])
				{
					temp=a[j];
					a[j]=a[k];
					a[k]=temp;
				}
				
			}
			j=0;
		}
	}
	

}
